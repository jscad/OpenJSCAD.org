{
  "source" : {
    "include": [
      "packages/array-utils/src",

      "packages/modeling/src/colors",
      "packages/modeling/src/curves",
      "packages/modeling/src/curves/bezier",
      "packages/modeling/src/geometries",
      "packages/modeling/src/geometries/geom2",
      "packages/modeling/src/geometries/geom3",
      "packages/modeling/src/geometries/path2",
      "packages/modeling/src/geometries/poly2",
      "packages/modeling/src/geometries/poly3",
      "packages/modeling/src/geometries/slice",
      "packages/modeling/src/maths",
      "packages/modeling/src/maths/line2",
      "packages/modeling/src/maths/line3",
      "packages/modeling/src/maths/mat4",
      "packages/modeling/src/maths/plane",
      "packages/modeling/src/maths/utils",
      "packages/modeling/src/maths/vec2",
      "packages/modeling/src/maths/vec3",
      "packages/modeling/src/maths/vec4",
      "packages/modeling/src/measurements",
      "packages/modeling/src/operations/booleans",
      "packages/modeling/src/operations/offsets",
      "packages/modeling/src/operations/extrusions",
      "packages/modeling/src/operations/hulls",
      "packages/modeling/src/operations/modifiers",
      "packages/modeling/src/operations/transforms",
      "packages/modeling/src/primitives",
      "packages/modeling/src/text",
      "packages/modeling/src/utils",

      "packages/io/io-utils",
      "packages/io/amf-deserializer/src",
      "packages/io/amf-serializer",
      "packages/io/svg-deserializer/src",
      "packages/io/svg-serializer",
      "packages/io/x3d-deserializer/src",
      "packages/io/x3d-serializer/src",
      "packages/io/3mf-deserializer/src",
      "packages/io/3mf-serializer/src"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": ".+test.js$"
  },
  "opts": {
    "template": "node_modules/docdash",
    "destination": "./docs",
    "tutorials": "./jsdoc/tutorials",
    "readme": "./jsdoc/assets/README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "staticFiles": {
        "include": [
          "./jsdoc/assets/static"
        ]
      }
    }
  },
  "docdash": {
    "static": [true],
    "sort": [true],
    "sectionOrder": [
      "Tutorials",
      "Modules",
      "Classes",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Interfaces"
    ],
    "disqus": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "JSCAD API Documentation",
      "description": "JSCAD API Documentation",
      "keyword": "JSCAD OpenJSCAD"
    },
    "search": [true],
    "collapse": [false],
    "wrap": [false],
    "typedefs": [true],
    "navLevel": [-1],
    "private": [true],
    "removeQuotes": ["none"],
    "scripts": ["jscad-docs.css", "jscad-docs.js"],
    "menu": {
      "JSCAD User Group": {
        "href":"https://openjscad.xyz/forum.html",
        "target":"_blank",
        "class":"menu-item",
        "id":"forum_link"
      },
      "JSCAD Github": {
        "href":"https://github.com/jscad/OpenJSCAD.org",
        "target":"_blank",
        "class":"menu-item",
        "id":"forum_link"
      }
    },
    "scopeInOutputPath": [true],
    "nameInOutputPath": [true],
    "versionInOutputPath": [true]
  }
}
