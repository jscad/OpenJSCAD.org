    },
    {
      "name": "Eduard Bespalov",
      "url": "http://evanw.github.com/csg.js/"
    },
    {
      "name": "bebbi",
      "email": "elghatta@gmail.com"
    },
    {
      "name": "Spiritdude Rene K Mueller",
      "url": "http://renekmueller.com"
    },
    {
      "name": "Jeff Gay",
      "url": "http://www.z3d.jp"
    }
  ],
  "keywords": [
    "csg",
    "parametric",
    "modeling",
    "openjscad",
    "jscad"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "ava": "3.10.0",
    "browserify": "16.5.1",
    "nyc": "15.1.0",
    "uglifyify": "5.0.2"
  }
}
